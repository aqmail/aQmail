#!/bin/sh
# WARNING: This file was auto-generated. Do not edit!
# POSIX compliant usage of options.
cd /var/qmail
cd queue
for dir in mess info local remote todo
do
  ( cd $dir; find . -type f -print ) | (
    cd $dir
    while read path
    do
      id=`basename "$path"`
      sub=`expr "$id" % 23`
      mv "$path" "$sub"/"$id"
    done
  )
done
